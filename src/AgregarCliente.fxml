      <Label layoutX="11.0" layoutY="14.0" text="Nombre:" textFill="#476072" />
      <Label layoutX="15.0" layoutY="73.0" text="ID (rut):" textFill="#476072" />
      <Button fx:id="agregar" layoutX="131.0" layoutY="140.0" mnemonicParsing="false" onAction="#handleAgregar" styleClass="backButton" stylesheets="@Styles.css" text="Agregar" textFill="#eeeeee" />
   </children>
